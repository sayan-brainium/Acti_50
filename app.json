    "android": {
      "package": "com.example.Acti_50",
      "splash": {
        "backgroundColor": "#ffffff",
        "resizeMode": "cover",
        "mdpi": "./assets/images/splash1.png",
        "hdpi": "./assets/images/splash2.png",
        "xhdpi": "./assets/images/splash3.png",
        "xxhdpi": "./assets/images/splash4.png",
        "xxxhdpi": "./assets/images/splash5.png"
      }
  }
  }
}